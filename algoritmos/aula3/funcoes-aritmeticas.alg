algoritmo "funcoes aritmeticas"
var
   a: inteiro
   b: real
inicio
      a <- abs(-50)
      escreval(a)
      
      b <- exp(9,5)
      escreval(b)
      
      b <- raizq(90)
      escreval(b)
      
      b <- int(raizq(90))
      escreval(b)
      
      b <- grauprad(90)
      escreval(b)
      
      b <- sen(grauprad(90))
      escreval(b)
fimalgoritmo
